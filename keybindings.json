// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "alt+j alt+j",
        "command": "cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "alt+k alt+k",
        "command": "cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "alt+; alt+;",
        "command": "cursorRight",
        "when": "textInputFocus"
    },
    {
        "key": "alt+l alt+l",
        "command": "cursorUp",
        "when": "textInputFocus"
    },
    {
        "key": "alt+m alt+;",
        "command": "cursorEnd",
        "when": "textInputFocus"
    },
    {
        "key": "alt+m alt+j",
        "command": "cursorHome",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+j shift+alt+j",
        "command": "cursorLeftSelect",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+k shift+alt+k",
        "command": "cursorDownSelect",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+l shift+alt+l",
        "command": "cursorUpSelect",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+; shift+alt+;",
        "command": "cursorRightSelect",
        "when": "textInputFocus"
    },
    {
        "key": "alt+m alt+k",
        "command": "cursorBottom",
        "when": "textInputFocus"
    },
    {
        "key": "alt+m alt+l",
        "command": "cursorTop",
        "when": "textInputFocus"
    }
]